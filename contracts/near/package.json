{
  "name": "groundone_part",
  "version": "1.0.0",
  "description": "Contract template with near-sdk-js",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "=====SETUP=====": "",
    "_____combination_____": "",
    "setup:clean": "yarn acc:delete && yarn acc:create && yarn acc:send && yarn build:deploy",
    "setup:getTimes": "./src/scripts/get_times.sh 0 5 10",
    "setup:presale:init": "yarn call:init:default && yarn call:presale:participate && yarn call:presale:participate2",
    "setup:presale:distribute": "yarn call:presale:distribute && yarn call:presale:cashout && yarn call:presale:mint",
    "setup:sale:init": "yarn call:init:presaleended && yarn setup:sale:mint",
    "setup:sale:mint": "yarn call:sale:mint && yarn call:sale:mint:buyer",
    "setup:property:init": "yarn call:property:init && yarn call:property:preference && call:property:preference2",
    "setup:property:distribute": "yarn call:property:distribute",
    "_____accounts_____": "",
    "acc:create": "source .env && yarn near create-account $PART_CONTRACT --masterAccount $MAIN_ACCOUNT --initialBalance 1",
    "acc:send": "source .env && yarn near send $MAIN_ACCOUNT $PART_CONTRACT 5",
    "acc:state": "source .env && yarn near state $PART_CONTRACT",
    "acc:delete": "source .env && yarn near delete $PART_CONTRACT $MAIN_ACCOUNT",
    "=====BUILD_DEPLOY=====": "",
    "build:deploy": "yarn build && yarn deploy:part",
    "build": "yarn build:part",
    "build:part": "near-sdk-js build src/part-contract/index.ts build/part.wasm",
    "deploy": "yarn deploy:part",
    "deploy:part": "source .env && near deploy --accountId $PART_CONTRACT --wasmFile build/part.wasm",
    "deploy:part:dev": "near dev-deploy build/part.wasm",
    "=====CALL_FUNCTIONS=====": "",
    "_____init_____": "",
    "call:init:default": "source .env && near call $PART_CONTRACT init '{\"ownerId\": \"'$PART_CONTRACT'\", \"projectName\": \"GroundOne Part\", \"totalSupply\": 4, \"price\": \"1000000000000000000000\", \"reservedTokenIds\": [\"2\"]}' --accountId $PART_CONTRACT --depositYocto 8880000000000000000000",
    "call:init:presaleended": "source .env && near call $PART_CONTRACT init '{\"ownerId\": \"'$PART_CONTRACT'\", \"projectName\": \"GroundOne Part\", \"totalSupply\": 4, \"price\": \"1000000000000000000000\", \"reservedTokenIds\": [\"2\"], \"saleOpening\": \"'$saleOpening'\", \"saleClose\": \"'$saleClose'\"}' --accountId $PART_CONTRACT --depositYocto 8880000000000000000000",
    "call:init:saleended": "source .env && near call $PART_CONTRACT init '{\"ownerId\": \"'$PART_CONTRACT'\", \"projectName\": \"GroundOne Part\", \"totalSupply\": 4, \"price\": \"1000000000000000000000\", \"reservedTokenIds\": [\"2\"], \"saleEnd\": 1}' --accountId $PART_CONTRACT --depositYocto 8880000000000000000000",
    "_____presale_____": "",
    "call:presale:participate": "source .env && near call $PART_CONTRACT participate_presale --accountId $MAIN_ACCOUNT --depositYocto 8880000000000000000000",
    "call:presale:participate2": "source .env && near call $PART_CONTRACT participate_presale --accountId $BUYER_ACCOUNT --depositYocto 8880000000000000000000",
    "call:presale:distribute": "source .env && near call $PART_CONTRACT distribute_after_presale --accountId $PART_CONTRACT",
    "call:presale:cashout": "source .env && near call $PART_CONTRACT cashout_unlucky_presale_participants --accountId $PART_CONTRACT",
    "call:presale:mint": "source .env && near call $PART_CONTRACT mint_for_presale_participants '{\"metadata\": {\"title\": \"GroundOne PART Token\", \"description\": \"Token ID is your ranking.\", \"media\": \"https://bafybeiftczwrtyr3k7a2k4vutd3amkwsmaqyhrdzlhvpt33dyjivufqusq.ipfs.dweb.link/goteam-gif.gif\"}}' --accountId $PART_CONTRACT --depositYocto 8880000000000000000000",
    "_____sale_____": "",
    "call:sale:mint": "source .env && near call $PART_CONTRACT nft_mint '{\"metadata\": {\"title\": \"GroundOne PART Token\", \"description\": \"Token ID is your ranking.\", \"media\": \"https://bafybeiftczwrtyr3k7a2k4vutd3amkwsmaqyhrdzlhvpt33dyjivufqusq.ipfs.dweb.link/goteam-gif.gif\"}, \"receiver_id\": \"'$MAIN_ACCOUNT'\"}' --accountId $MAIN_ACCOUNT --amount 0.018",
    "call:sale:mint:buyer": "source .env && near call $PART_CONTRACT nft_mint '{\"metadata\": {\"title\": \"GroundOne PART Token\", \"description\": \"Token ID is your ranking.\", \"media\": \"https://bafybeiftczwrtyr3k7a2k4vutd3amkwsmaqyhrdzlhvpt33dyjivufqusq.ipfs.dweb.link/goteam-gif.gif\"}, \"receiver_id\": \"'$BUYER_ACCOUNT'\"}' --accountId $BUYER_ACCOUNT --amount 0.018",
    "_____property_____": "",
    "call:property:init": "source .env && near call $PART_CONTRACT init_property '{\"totalSupply\": 2, \"distributionStart\": \"'$distributionStart'\", \"reservedTokenIds\": [\"2\"]}' --accountId $PART_CONTRACT",
    "call:property:preference": "source .env && near call $PART_CONTRACT set_preferences_properties '{\"propertyPreferenceIds\": [\"2\", \"1\"]}' --accountId $MAIN_ACCOUNT",
    "call:property:preference2": "source .env && near call $PART_CONTRACT set_preferences_properties '{\"propertyPreferenceIds\": [\"1\"]}' --accountId $BUYER_ACCOUNT",
    "call:property:distribute": "source .env && near call $PART_CONTRACT distribute_properties '{\"propertyPreferenceIds\": [\"1\"]}' --accountId $PART_CONTRACT",
    "=====VIEW FUNCTIONS=====": "",
    "_____general_____": "",
    "view:meta": "source .env && near view $PART_CONTRACT nft_metadata",
    "view:vars": "source .env && near view $PART_CONTRACT contract_vars",
    "view:property_vars": "source .env && near view $PART_CONTRACT property_vars",
    "view:blocktime": "source .env && near view $PART_CONTRACT current_block_time",
    "view:done:presale": "source .env && near view $PART_CONTRACT isPresaleDone",
    "view:done:sale": "source .env && near view $PART_CONTRACT isSaleDone",
    "_____token_____": "",
    "view:token": "source .env && near view $PART_CONTRACT nft_token '{\"token_id\": \"1\"}'",
    "view:tokens": "source .env && near view $PART_CONTRACT nft_tokens",
    "view:tokens:owner": "source .env && near view $PART_CONTRACT nft_tokens_for_owner '{\"account_id\": \"groundone.testnet\"}'",
    "view:property:token": "source .env && near view $PART_CONTRACT property_info '{\"id\": \"1\"}'",
    "view:property:tokens": "source .env && near view $PART_CONTRACT properties_info",
    "_____v_presale_____": "",
    "view:presale:participants": "source .env && near view $PART_CONTRACT presale_participants",
    "view:presale:distribution": "source .env && near view $PART_CONTRACT presale_distribution",
    "=====TESTING=====": "",
    "test": "ava",
    "test:watch": "nodemon --exec 'yarn test' -w ./__tests__ -w ./src -e js,ts",
    "build:test": "yarn build && yarn test"
  },
  "author": "Tobias Leinss <caruso33@web.de>, Frangiskos Frangeskides <frangiskos@gmail.com>",
  "license": "UNLICENSED",
  "dependencies": {
    "lodash-es": "^4.17.21",
    "near-sdk-js": "0.5.0"
  },
  "devDependencies": {
    "@types/node": "^18.7.23",
    "ava": "^4.3.3",
    "near-cli": "^3.4.2",
    "near-workspaces": "^3.2.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
