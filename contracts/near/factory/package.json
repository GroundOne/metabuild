{
  "name": "factory",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "=====SETUP=====": "",
    "_____combination_____": "",
    "setup:clean": "yarn acc:delete && yarn acc:create && yarn acc:send && yarn build:deploy",
    "_____accounts_____": "",
    "acc:create": "source .env && yarn near create-account $FACTORY_CONTRACT --masterAccount $MAIN_ACCOUNT",
    "acc:send": "source .env && yarn near send $MAIN_ACCOUNT $FACTORY_CONTRACT 4",
    "acc:state": "source .env && yarn near state $FACTORY_CONTRACT",
    "acc:delete": "source .env && yarn near delete $FACTORY_CONTRACT $MAIN_ACCOUNT",
    "=====BUILD_DEPLOY=====": "",
    "build:deploy": "yarn build && yarn deploy:part",
    "build": "./build.sh",
    "deploy": "source .env && near deploy --accountId $FACTORY_CONTRACT --wasmFile res/token_factory.wasm",
    "deploy:dev": "near dev-deploy build/token_factory.wasm"
  },
  "devDependencies": {
    "near-cli": "^3.4.2"
  }
}
