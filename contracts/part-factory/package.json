{
  "name": "factory",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "=====SETUP=====": "",
    "_____combination_____": "",
    "setup:clean": "yarn acc:delete && yarn acc:create && yarn acc:send && yarn build:deploy",
    "_____accounts_____": "",
    "acc:create": "source .env && yarn near create-account $FACTORY_CONTRACT --masterAccount $MAIN_ACCOUNT",
    "acc:send": "source .env && yarn near send $MAIN_ACCOUNT $ARCHITECT_ACCOUNT 6",
    "acc:state": "source .env && yarn near state $FACTORY_CONTRACT",
    "acc:delete": "source .env && yarn near delete $FACTORY_CONTRACT $MAIN_ACCOUNT",
    "=====BUILD_DEPLOY=====": "",
    "build:deploy": "yarn build && yarn deploy",
    "build": "./scripts/build.sh",
    "deploy": "source .env && near deploy --accountId $FACTORY_CONTRACT --wasmFile res/part_token_factory.wasm",
    "deploy:dev": "near dev-deploy res/part_token_factory.wasm",
    "=====CALL_FUNCTIONS=====": "",
    "_____init_____": "",
    "call:new": "source .env && near call $FACTORY_CONTRACT new --accountId $MAIN_ACCOUNT",
    "call:create": "source .env && near call $FACTORY_CONTRACT create_token '{\"args\": {\"projectAddress\": \"token_project_test12\", \"projectName\": \"Test Project\", \"ownerId\": \"'$ARCHITECT_ACCOUNT'\", \"totalSupply\": 4, \"price\": \"'$TOKEN_PRICE'\", \"reservedTokenIds\": [\"2\"],  \"prelaunchEnd\": \"'$saleOpening'\", \"saleEnd\": \"'$saleClose'\", \"metadata\": {\"spec\": \"nft-1.0.0\", \"name\": \"GroundOne PART\", \"symbol\": \"GOPART\"}}}' --accountId $ARCHITECT_ACCOUNT --depositYocto $DEPOSIT --gas $GAS",
    "=====VIEW FUNCTIONS=====": "",
    "view:nrtokens": "source .env && near call $FACTORY_CONTRACT get_number_of_tokens --accountId $ARCHITECT_ACCOUNT",
    "view:contracts": "source .env && near call $FACTORY_CONTRACT get_contracts --accountId $ARCHITECT_ACCOUNT",
    "view:contract": "source .env && near call $FACTORY_CONTRACT get_contract '{\"token_id\": \"'$FACTORY_CONTRACT'\"}' --accountId $ARCHITECT_ACCOUNT",
    "view:token:supply": "source .env && near view $FACTORY_CONTRACT supply_for_owner '{\"account_id\": \"'$ARCHITECT_ACCOUNT'\"}' --accountId $ARCHITECT_ACCOUNT",
    "view:token:owner": "source .env && near view $FACTORY_CONTRACT contracts_for_owner '{\"account_id\": \"'$ARCHITECT_ACCOUNT'\"}' --accountId $ARCHITECT_ACCOUNT"
  },
  "devDependencies": {
    "dotenv": "^16.0.3",
    "near-api-js": "^1.1.0",
    "near-cli": "^3.4.2"
  }
}
