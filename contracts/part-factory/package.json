{
  "name": "factory",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "=====SETUP=====": "",
    "_____combination_____": "",
    "setup:clean": "yarn acc:delete && yarn acc:create && yarn acc:send && yarn build:deploy",
    "_____accounts_____": "",
    "acc:create": "source .env && yarn near create-account $FACTORY_CONTRACT --masterAccount $MAIN_ACCOUNT",
    "acc:send": "source .env && yarn near send $MAIN_ACCOUNT $ARCHITECT_ACCOUNT 6",
    "acc:state": "source .env && yarn near state $FACTORY_CONTRACT",
    "acc:delete": "source .env && yarn near delete $FACTORY_CONTRACT $MAIN_ACCOUNT",
    "=====BUILD_DEPLOY=====": "",
    "build:deploy": "yarn build && yarn deploy",
    "build": "./scripts/build.sh",
    "deploy": "source .env && near deploy --accountId $FACTORY_CONTRACT --wasmFile res/part_token_factory.wasm",
    "deploy:dev": "near dev-deploy build/part_token_factory.wasm",
    "=====FACTORY_CONTRACT=====": "",
    "=====CALL_FUNCTIONS=====": "",
    "_____init_____": "",
    "call:new": "source .env && near call $FACTORY_CONTRACT new --accountId $MAIN_ACCOUNT",
    "call:create": "source .env && near call $FACTORY_CONTRACT create_token '{\"args\": {\"ownerId\": \"'$ARCHITECT_ACCOUNT'\", \"projectName\": \"Part Test3 Project\", \"totalSupply\": \"4\", \"price\": \"1000000000000000000000\", \"reservedTokenIds\": [\"2\"], \"prelaunchEnd\": \"'$saleOpening'\", \"saleEnd\": \"'$saleClose'\", \"metadata\": {\"spec\": \"nft-1.0.0\", \"name\": \"GroundOne PART\", \"symbol\": \"GOPART\"}}}' --accountId $ARCHITECT_ACCOUNT --depositYocto 5700000000000000000000000 --gas=75000000000000",
    "=====VIEW FUNCTIONS=====": "",
    "view:nrtokens": "source .env && near call $FACTORY_CONTRACT get_number_of_tokens --accountId $ARCHITECT_ACCOUNT",
    "view:contracts": "source .env && near call $FACTORY_CONTRACT get_contracts --accountId $ARCHITECT_ACCOUNT",
    "view:contract": "source .env && near call $FACTORY_CONTRACT get_contract '{\"token_id\": \"part_test3_project\"}' --accountId $ARCHITECT_ACCOUNT",
    "view:token:supply": "source .env && near view $FACTORY_CONTRACT supply_for_owner '{\"account_id\": \"'$ARCHITECT_ACCOUNT'\"}' --accountId $ARCHITECT_ACCOUNT",
    "view:token:owner": "source .env && near view $FACTORY_CONTRACT contracts_for_owner '{\"account_id\": \"'$ARCHITECT_ACCOUNT'\"}' --accountId $ARCHITECT_ACCOUNT",
    "=====PART_TOKEN_CONTRACT=====": "",
    "=====PART_TOKEN_CALL_FUNCTIONS=====": "",
    "_____part_token_init_____": "",
    "call:init:default": "source .env && near call $PART_CONTRACT init '{\"ownerId\": \"'$ARCHITECT_ACCOUNT'\", \"projectName\": \"GroundOne Part\", \"totalSupply\": 4, \"price\": \"1000000000000000000000\", \"reservedTokenIds\": [\"2\"]}' --accountId $ARCHITECT_ACCOUNT --depositYocto 8880000000000000000000",
    "call:init:presaleended": "source .env && near call $PART_CONTRACT init '{\"ownerId\": \"'$ARCHITECT_ACCOUNT'\", \"projectName\": \"GroundOne Part\", \"totalSupply\": 4, \"price\": \"1000000000000000000000\", \"reservedTokenIds\": [\"2\"], \"saleOpening\": \"'$saleOpening'\", \"saleClose\": \"'$saleClose'\"}' --accountId $ARCHITECT_ACCOUNT --depositYocto 8880000000000000000000",
    "call:init:saleended": "source .env && near call $PART_CONTRACT init '{\"ownerId\": \"'$ARCHITECT_ACCOUNT'\", \"projectName\": \"GroundOne Part\", \"totalSupply\": 4, \"price\": \"1000000000000000000000\", \"reservedTokenIds\": [\"2\"], \"saleEnd\": 1}' --accountId $ARCHITECT_ACCOUNT --depositYocto 8880000000000000000000",
    "_____presale_____": "",
    "call:presale:participate": "source .env && near call $PART_CONTRACT participate_presale --accountId $MAIN_ACCOUNT --depositYocto 8880000000000000000000",
    "call:presale:participate2": "source .env && near call $PART_CONTRACT participate_presale --accountId $BUYER_ACCOUNT --depositYocto 8880000000000000000000",
    "call:presale:distribute": "source .env && near call $PART_CONTRACT distribute_after_presale --accountId $ARCHITECT_ACCOUNT",
    "call:presale:cashout": "source .env && near call $PART_CONTRACT cashout_unlucky_presale_participants --accountId $ARCHITECT_ACCOUNT",
    "call:presale:mint": "source .env && near call $PART_CONTRACT mint_for_presale_participants '{\"metadata\": {\"title\": \"GroundOne PART Token\", \"description\": \"Token ID is your ranking.\", \"media\": \"https://bafybeiftczwrtyr3k7a2k4vutd3amkwsmaqyhrdzlhvpt33dyjivufqusq.ipfs.dweb.link/goteam-gif.gif\"}}' --accountId $ARCHITECT_ACCOUNT --depositYocto 8880000000000000000000",
    "_____sale_____": "",
    "call:sale:mint": "source .env && near call $PART_CONTRACT nft_mint '{\"metadata\": {\"title\": \"GroundOne PART Token\", \"description\": \"Token ID is your ranking.\", \"media\": \"https://bafybeiftczwrtyr3k7a2k4vutd3amkwsmaqyhrdzlhvpt33dyjivufqusq.ipfs.dweb.link/goteam-gif.gif\"}, \"receiver_id\": \"'$MAIN_ACCOUNT'\"}' --accountId $MAIN_ACCOUNT --amount 0.018",
    "call:sale:mint:buyer": "source .env && near call $PART_CONTRACT nft_mint '{\"metadata\": {\"title\": \"GroundOne PART Token\", \"description\": \"Token ID is your ranking.\", \"media\": \"https://bafybeiftczwrtyr3k7a2k4vutd3amkwsmaqyhrdzlhvpt33dyjivufqusq.ipfs.dweb.link/goteam-gif.gif\"}, \"receiver_id\": \"'$BUYER_ACCOUNT'\"}' --accountId $BUYER_ACCOUNT --amount 0.018",
    "_____property_____": "",
    "call:property:init": "source .env && near call $PART_CONTRACT init_property '{\"totalSupply\": 2, \"distributionStart\": \"'$distributionStart'\", \"reservedTokenIds\": [\"2\"]}' --accountId $ARCHITECT_ACCOUNT",
    "call:property:preference": "source .env && near call $PART_CONTRACT set_preferences_properties '{\"propertyPreferenceIds\": [\"2\", \"1\"]}' --accountId $MAIN_ACCOUNT",
    "call:property:preference2": "source .env && near call $PART_CONTRACT set_preferences_properties '{\"propertyPreferenceIds\": [\"1\"]}' --accountId $BUYER_ACCOUNT",
    "call:property:distribute": "source .env && near call $PART_CONTRACT distribute_properties '{\"propertyPreferenceIds\": [\"1\"]}' --accountId $ARCHITECT_ACCOUNT",
    "=====PART_TOKEN_VIEW FUNCTIONS=====": "",
    "_____general_____": "",
    "view:meta": "source .env && near view $PART_CONTRACT nft_metadata",
    "view:vars": "source .env && near view $PART_CONTRACT contract_vars",
    "view:property_vars": "source .env && near view $PART_CONTRACT property_vars",
    "view:blocktime": "source .env && near view $PART_CONTRACT current_block_time",
    "view:done:presale": "source .env && near view $PART_CONTRACT isPresaleDone",
    "view:done:sale": "source .env && near view $PART_CONTRACT isSaleDone",
    "_____token_____": "",
    "view:token": "source .env && near view $PART_CONTRACT nft_token '{\"token_id\": \"1\"}'",
    "view:tokens": "source .env && near view $PART_CONTRACT nft_tokens",
    "view:tokens:owner": "source .env && near view $PART_CONTRACT nft_contracts_for_owner '{\"account_id\": \"groundone.testnet\"}'",
    "view:property:token": "source .env && near view $PART_CONTRACT property_info '{\"id\": \"1\"}'",
    "view:property:tokens": "source .env && near view $PART_CONTRACT properties_info",
    "_____v_presale_____": "",
    "view:presale:participants": "source .env && near view $PART_CONTRACT presale_participants",
    "view:presale:distribution": "source .env && near view $PART_CONTRACT presale_distribution"
  },
  "devDependencies": {
    "dotenv": "^16.0.3",
    "near-api-js": "^1.1.0",
    "near-cli": "^3.4.2"
  }
}
